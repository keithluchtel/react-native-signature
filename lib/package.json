{
  "name": "@noxother/signature",
  "version": "1.0.0",
  "author": "Keith Luchtel",
  "private": false,
  "description": "A react native signature canvas built on skia",
  "repository": {
    "type": "git",
    "url": "https://github.com/keithluchtel/react-native-signature"
  },
  "main": "dist/commonjs/index.js",
  "scripts": {
    "prepare": "bob build"
  },
  "keywords": [
    "react-native",
    "signature",
    "signature-pad",
    "signature-canvas",
    "skia"
  ],
  "license": "MIT",
  "devDependencies": {
    "react-native-builder-bob": "^0.23.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "@shopify/react-native-skia": ">=0.1.227",
    "react-native-reanimated": ">=3.0.0",
    "react-native-gesture-handler": ">=2.0.0"
  },
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "module": "dist/module/index.js",
  "types": "dist/typescript/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "dist",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
